<% if current_user %>
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Log In', login_path %>
  <%= button_to "Create a New User", register_path, method: :get %>
<% end %>

<h3> Existing User </h3>

<% if current_admin %>
  <% @users.each do |user| %>
    <%= link_to user.email, admin_user_path(user) %></br>
  <% end %>
<% elsif current_user %>
  <% @users.each do |user| %>
    <%= user.email %></br>
  <% end %>
<% end %>